function src = create_dreemsrc(labels)


files = [];
classes = {'pos', 'neg'};

% objects = [];


parfor ind = 1: length(labels) %   files)
    
disp(['runing parfor ...i = ', int2str(ind)]);    
    new_object{ind}.ind = ind;
    new_object{ind}.u1 = 1;
    new_object{ind}.u2 = 600;
    new_object{ind}.cl = labels(ind)*2 - 1;
    
end
disp('done parfor');

parfor  ind = 1 : length(labels)
   
    objects(i) = [objects, new_object{ind}];
end


% for  ind = 1 : length(labels)
%     disp(['runing for ...i = ', int2str(ind)]);    
%     new_object.ind = ind;
%     new_object.u1 = 1; new_object.u2 = 600;
%     new_object.class = labels(ind)*2 - 1;
% 
%     objects = [objects, new_object];
% end



src.classes = classes;
src.files = files;
src.objects = objects;
end
